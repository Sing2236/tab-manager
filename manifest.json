{
    "manifest_version": 3,
    "name": "Tab Manager - Time Sensitivity",
    "version": "1.1",
    "description": "Manage tabs with time categories and view a persistent sidebar list.",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "sidebar_action": {
      "default_panel": "sidebar.html"
    }
  }
  